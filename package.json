{
  "name": "snd-nextjs-app",
  "description": "A Next.js boilerplate",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "yarn removeNextJsTmp && NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "yarn removeNextJsTmp && next build",
    "build-standalone": "yarn removeNextJsTmp && next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "start": "next start",
    "start-standalone": "PORT=3001 node .next/standalone/server.js",
    "lint": "next lint",
    "removeNextJsTmp": "rm -rf ./.next",
    "removeNodeModules": "rm -rf ./node_modules"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.2",
    "@mui/material-nextjs": "^7.0.2",
    "@tailwindcss/postcss": "^4.0.15",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "culori": "^4.0.1",
    "next": "^15.2.3",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-json-view": "^1.21.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/culori": "^2.1.1",
    "@types/node": "^22.14.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15.2.3",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "engines": {
    "node": ">=19.0.0 <=22.x.x"
  }
}
